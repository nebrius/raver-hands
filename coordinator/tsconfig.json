{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "sourceMap": false,
        "outDir": "dist",
        "strictNullChecks": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "typeRoots": [
            "./node_modules/@types",
            "./typings-custom"
        ]
    },
    "include": [
        "src/**/*.ts"
    ]
}
